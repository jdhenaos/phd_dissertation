{
  "name": "R rrtools Environment",
  "build": {
    "dockerfile": "./Dockerfile",
    "context": ".."
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "REditorSupport.r",
        "quarto.quarto",
        "ms-python.python"
      ]
    }
  },
  "remoteUser": "root",
  "mounts": [
    "source=${localWorkspaceFolder},target=/project,type=bind"
  ]
}

